<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 다른 mapper와 중복되지 않도록 네임스페이스 기재 -->
<mapper namespace="member">
<select id="memberList" resultType="com.example.trakker.model.member.dto.MemberDTO">
select*from member
order by mem_num desc
</select>
<select id="memberView"  resultType="com.example.trakker.model.member.dto.MemberDTO">
select *from member
where mem_num =#{mem_num}
</select>
</mapper>














