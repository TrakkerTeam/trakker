<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 다른 mapper와 중복되지 않도록 네임스페이스 기재 -->
<mapper namespace="com.example.trakker.item.dao.ItemDAO">

    <resultMap id="resultItem" type="com.example.trakker.item.entity.ItemEntity">
        <result property="pageNum" column="page_num" />
        <result property="recordNum" column="record_num" />
        <result property="pageTotalCount" column="page_totalcount" />


        <result property="iidx" column="iidx" />
        <result property="title" column="title" />
        <result property="description" column="description" />
        <result property="author" column="author" />
    </resultMap>




</mapper>














